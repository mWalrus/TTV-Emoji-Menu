{
  "manifest_version": 2,
  "name": "TTVEmoji Menu",
  "version": "1.8.5",
  "description": "An emoji menu for twitch.tv",
  "content_scripts": [
    {
      "matches": ["*://*.twitch.tv/*"],
      "js": [
        "emojis.js",
        "comboSpan.js",
        "comboStorage.js",
        "menu.js",
        "manageCombos.js",
        "ttvemojimenu.js"
      ],
      "css": ["css/main.css"]
    }
  ],
  "icons": {
    "512": "/img/face.png"
  },
  "web_accessible_resources": ["img/settings_symbol.png"]
}
